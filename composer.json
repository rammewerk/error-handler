{
  "name": "rammewerk/error-handler",
  "type": "library",
  "description": "Simple class to manage and normalize PHP errors",
  "homepage": "https://rammewerk.com",
  "license": "MIT",
  "keywords": [
    "error-handler",
    "exceptions"
  ],
  "authors": [
    {
      "name": "Kristoffer Follestad",
      "email": "kristoffer@rammewerk.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rammewerk\\Component\\ErrorHandler\\": "src"
    },
    "exclude-from-classmap": [
      "/tests/",
      "/.idea"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rammewerk\\Component\\ErrorHandler\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2"
  }
}